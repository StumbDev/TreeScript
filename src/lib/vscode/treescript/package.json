{
    "name": "treescript",
    "displayName": "TreeScript",
    "description": "TreeScript support for vscode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.93.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "treescript",
            "aliases": [
                "TreeScript",
                "treescript"
            ],
            "extensions": [
                ".voi",
                ".ctscr"
            ],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "treescript",
            "scopeName": "source.ctscr",
            "path": "./syntaxes/treescript.tmLanguage.json"
        }],
        "fileIcons": {
            "*.voi": "./assents/icon.svg",
            "*.cts": "./assents/icon.svg",
            "ctsconfig.json": "./assents/configfile.svg"
        }
    },
    "dependencies": {
        "vsce": "^2.15.0"
    }
}