{
    "name": "treescript",
    "displayName": "TreeScript",
    "description": "TreeScript support for vscode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.93.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "treescript",
            "aliases": ["TreeScript", "treescript"],
            "extensions": [".voi", ".cts"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "treescript",
            "scopeName": "source.cts",
            "path": "./syntaxes/treescript.tmLanguage.json"
        }],
        "fileIcons": {
            "*.voi": "./assents/icon.svg",
            "*.cts": "./assents/icon.svg",
            "ctsconfig.json": "./assents/configfile.svg"
        }
    }
}